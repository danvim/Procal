/* Cubic Equation */
?->$a: ?->$b: ?->$c: ?->$d:
(3$a$c - $b^2)/(9($a^2)) -> $Q:
(9$a$b$c - 27*($a^2)*$d - 2*($b^3))/(54($a^3)) -> $R:
$Q display
$R display
acos($R/sqrt((-$Q)^3)) -> $t:
$t display
2sqrt(-$Q)cos($t/3) - $b/(3$a) display
2sqrt(-$Q)cos($t/3 + 2&pi/3) - $b/(3$a) display
2sqrt(-$Q)cos($t/3 + 4&pi/3) - $b/(3$a) display
