/* Cubic Equation */
?->$A: ?->$B: ?->$C: ?->$D:
(-$B)/(3$A) -> $B:
$C/$A -> $C:
$B^3 - $B$C/2 - $D/(2$A) -> $D:
$B^2 - $C/3 -> $Y:
$D^2 - $Y^3 -> $X:
If $X>0 Then
	$D + sqrt($X):
	(Ans)^(1/3) - (Ans - 2$D)^(1/3):
Else
	If $Y Then
		2sqrt($Y)cos(1/3 * acos($D/sqrt($Y^3))):
	IfEnd:
IfEnd:
$B + Ans -> $A:
$A display
3$B - $A -> $D:
$D/2 + sqrt($D^2 / 4 - $C + $A$D) -> $B:
$B display
$D - $B